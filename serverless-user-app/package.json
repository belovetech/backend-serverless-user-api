{
  "name": "serverless-user-app",
  "version": "1.0.0",
  "description": "Serverless user app",
  "main": "serverless.ts",
  "scripts": {
    "test": "./node_modules/.bin/mocha --require ts-node/register tests/**/*.ts",
    "start": "./node_modules/.bin/serverless --offline start --stage dev",
    "dev": "nodemon ./node_modules/.bin/serverless --offline start --stage dev",
    "build": "./node_modules/.bin/serverless build --stage dev",
    "lint": "./node_modules/.bin/eslint --ext .ts .",
    "lint:fix": "./node_modules/.bin/eslint --fix",
    "prepare": "husky install"
  },
  "keywords": [
    "backend",
    "serverless",
    "lambda"
  ],
  "author": "Abeeb Raheem",
  "license": "ISC",
  "dependencies": {
    "@middy/core": "^4.6.0",
    "@middy/http-json-body-parser": "^4.6.0",
    "dotenv": "^16.3.1",
    "serverless-dynamodb-client": "^0.0.2",
    "serverless-dynamodb-local": "^0.2.40"
  },
  "devDependencies": {
    "@serverless/typescript": "^3.30.1",
    "@types/aws-lambda": "^8.10.119",
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "husky": "^8.0.3",
    "json-schema-to-ts": "^2.9.2",
    "nodemon": "^3.0.1",
    "serverless-bundle": "^6.0.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.46.0",
    "serverless-localstack": "^1.1.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
